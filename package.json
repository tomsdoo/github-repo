{
  "name": "@tomsd/github-repo",
  "version": "1.1.0",
  "description": "Utility for GitHub repo",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
    "format": "prettier --write __test__/**/*.ts && prettier --write src/**/*.ts",
    "make:typedoc": "typedoc",
    "lint": "eslint __test__/**/*.ts && eslint src/**/*.ts",
    "prepare": "husky install",
    "serve:doc": "mdbook --serve --directory docs/",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "npm run format"
    ],
    "__test__/**/*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "keywords": [],
  "author": "tom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/github-repo.git"
  },
  "bugs": {
    "url": "https://github.com/tomsdoo/github-repo/issues"
  },
  "homepage": "https://github.com/tomsdoo/github-repo#readme",
  "dependencies": {
    "@octokit/rest": "^19.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@tomsd/md-book": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "babel-jest": "^29.3.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  }
}
